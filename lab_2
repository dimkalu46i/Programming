#include <stdio.h> 
#include <math.h> 
 
int main() 
{ 
    float z1 = 0, z2 = 0, z3 = 0, z4 = 0, z5 = 0 a = 0; 
    scanf("%f", &a); 
    z1 = (sin(2 * a) + sin(5 * a) - sin(3 * a)) / (cos(a) + 1 - 2 * pow(sin(2 * a), 2)); 
    printf("%f\n", z1); 
    z2 = 2 * sin(a); 
    printf("%f\n", z2); 
    z3 = sqrt(3); 
    printf("%f\n", z3); 
    z4 = round(z3 / z2); 
    printf("%f\n", z4); 
    z5 = log10(29); 
    printf("%f\n", z5); 
    return 0; 
}
